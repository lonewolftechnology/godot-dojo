<?xml version="1.0" encoding="UTF-8" ?>
<class name="DojoTokenQuery" inherits="DojoQueryBase" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/godotengine/godot/master/doc/class.xsd">
	<brief_description>
		A query for retrieving tokens.
	</brief_description>
	<description>
		This class is used to query for tokens (e.g., ERC721, ERC1155). It allows filtering by contract addresses and token IDs.
	</description>
	<tutorials>
	</tutorials>
	<members>
		<member name="contract_addresses" type="String[]" setter="set_contract_addresses" getter="get_contract_addresses" default="[]">
			An array of contract addresses to filter by.
		</member>
		<member name="token_ids" type="String[]" setter="set_token_ids" getter="get_token_ids" default="[]">
			An array of token IDs to filter by.
		</member>
	</members>
</class>
