<?xml version="1.0" encoding="UTF-8" ?>
<class name="TransactionSubscription" inherits="DojoSubscription" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/godotengine/godot/master/doc/class.xsd">
	<brief_description>
		A subscription for transaction updates.
	</brief_description>
	<description>
		This class is used to subscribe to transaction updates. It allows filtering by various criteria such as caller, contract, entrypoint, and block range.
	</description>
	<tutorials>
	</tutorials>
	<members>
		<member name="caller_addresses" type="String[]" setter="set_caller_addresses" getter="get_caller_addresses" default="[]">
			An array of caller addresses to filter by.
		</member>
		<member name="contract_addresses" type="String[]" setter="set_contract_addresses" getter="get_contract_addresses" default="[]">
			An array of contract addresses to filter by.
		</member>
		<member name="entrypoints" type="String[]" setter="set_entrypoints" getter="get_entrypoints" default="[]">
			An array of entrypoints to filter by.
		</member>
		<member name="filter_enabled" type="bool" setter="set_filter_enabled" getter="is_filter_enabled" default="false">
			Whether the filter is enabled.
		</member>
		<member name="from_block" type="int" setter="set_from_block" getter="get_from_block" default="-1">
			The starting block number to filter from.
		</member>
		<member name="model_selectors" type="String[]" setter="set_model_selectors" getter="get_model_selectors" default="[]">
			An array of model selectors to filter by.
		</member>
		<member name="name" type="String" setter="set_name" getter="get_name" overrides="DojoSubscription" default="&quot;TransactionSubscription&quot;" />
		<member name="to_block" type="int" setter="set_to_block" getter="get_to_block" default="-1">
			The ending block number to filter to.
		</member>
		<member name="transaction_hashes" type="String[]" setter="set_transaction_hashes" getter="get_transaction_hashes" default="[]">
			An array of transaction hashes to filter by.
		</member>
		<member name="type" type="int" setter="" getter="get_type" overrides="DojoSubscription" enum="DojoSubscription.Type" default="6" />
	</members>
</class>
