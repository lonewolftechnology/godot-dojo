<?xml version="1.0" encoding="UTF-8" ?>
<class name="DojoAggregationQuery" inherits="DojoQueryBase" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/godotengine/godot/master/doc/class.xsd">
	<brief_description>
		A query for retrieving aggregations.
	</brief_description>
	<description>
		This class is used to query for aggregations. It allows filtering by entity IDs and aggregator IDs.
	</description>
	<tutorials>
	</tutorials>
	<members>
		<member name="aggregator_ids" type="String[]" setter="set_aggregator_ids" getter="get_aggregator_ids" default="[]">
			An array of aggregator IDs to filter by.
		</member>
		<member name="entity_ids" type="String[]" setter="set_entity_ids" getter="get_entity_ids" default="[]">
			An array of entity IDs to filter by.
		</member>
	</members>
</class>
