<?xml version="1.0" encoding="UTF-8" ?>
<class name="ActivitySubscription" inherits="DojoSubscription" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/godotengine/godot/master/doc/class.xsd">
	<brief_description>
		A subscription for activity updates.
	</brief_description>
	<description>
		This class is used to subscribe to activity updates. It allows filtering by world addresses, caller addresses, and namespaces.
	</description>
	<tutorials>
	</tutorials>
	<members>
		<member name="caller_addresses" type="String[]" setter="set_caller_addresses" getter="get_caller_addresses" default="[]">
			An array of caller addresses to filter by.
		</member>
		<member name="name" type="String" setter="set_name" getter="get_name" overrides="DojoSubscription" default="&quot;ActivitySubscription&quot;" />
		<member name="namespaces" type="String[]" setter="set_namespaces" getter="get_namespaces" default="[]">
			An array of namespaces to filter by.
		</member>
		<member name="type" type="int" setter="" getter="get_type" overrides="DojoSubscription" enum="DojoSubscription.Type" default="10" />
		<member name="world_addresses" type="String[]" setter="set_world_addresses" getter="get_world_addresses" default="[]">
			An array of world addresses to filter by.
		</member>
	</members>
</class>
