		<constant name="ArrayLengthGt" value="13" enum="ComparisonOperator">
			Array length greater than operator.
		</constant>
		<constant name="ArrayLengthLt" value="14" enum="ComparisonOperator">
			Array length less than operator.
		</constant>
	</constants>
</class>
