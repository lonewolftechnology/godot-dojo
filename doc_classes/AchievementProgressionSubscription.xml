<?xml version="1.0" encoding="UTF-8" ?>
<class name="AchievementProgressionSubscription" inherits="DojoSubscription" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/godotengine/godot/master/doc/class.xsd">
	<brief_description>
		A subscription for achievement progression updates.
	</brief_description>
	<description>
		This class is used to subscribe to achievement progression updates. It allows filtering by world addresses, player addresses, achievement IDs, and namespaces.
	</description>
	<tutorials>
	</tutorials>
	<members>
		<member name="achievements_ids" type="String[]" setter="set_achievements_ids" getter="get_achievements_ids" default="[]">
			An array of achievement IDs to filter by.
		</member>
		<member name="name" type="String" setter="set_name" getter="get_name" overrides="DojoSubscription" default="&quot;AchievementProgressionSubscription&quot;" />
		<member name="namespaces" type="String[]" setter="set_namespaces" getter="get_namespaces" default="[]">
			An array of namespaces to filter by.
		</member>
		<member name="player_addresses" type="String[]" setter="set_player_addresses" getter="get_player_addresses" default="[]">
			An array of player addresses to filter by.
		</member>
		<member name="type" type="int" setter="" getter="get_type" overrides="DojoSubscription" enum="DojoSubscription.Type" default="11" />
		<member name="world_addresses" type="String[]" setter="set_world_addresses" getter="get_world_addresses" default="[]">
			An array of world addresses to filter by.
		</member>
	</members>
</class>
