<?xml version="1.0" encoding="UTF-8" ?>
<class name="TokenBalanceSubscription" inherits="DojoSubscription" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/godotengine/godot/master/doc/class.xsd">
	<brief_description>
		A subscription for token balance updates.
	</brief_description>
	<description>
		This class is used to subscribe to token balance updates. It allows filtering by account addresses, contract addresses, and token IDs.
	</description>
	<tutorials>
	</tutorials>
	<members>
		<member name="account_addresses" type="String[]" setter="set_account_addresses" getter="get_account_addresses" default="[]">
			An array of account addresses to filter by.
		</member>
		<member name="contract_addresses" type="String[]" setter="set_contract_addresses" getter="get_contract_addresses" default="[]">
			An array of contract addresses to filter by.
		</member>
		<member name="name" type="String" setter="set_name" getter="get_name" overrides="DojoSubscription" default="&quot;TokenBalanceSubscription&quot;" />
		<member name="token_ids" type="String[]" setter="set_token_ids" getter="get_token_ids" default="[]">
			An array of token IDs to filter by.
		</member>
		<member name="type" type="int" setter="" getter="get_type" overrides="DojoSubscription" enum="DojoSubscription.Type" default="4" />
	</members>
</class>
