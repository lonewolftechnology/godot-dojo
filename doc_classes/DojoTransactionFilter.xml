<?xml version="1.0" encoding="UTF-8" ?>
<class name="DojoTransactionFilter" inherits="Resource" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/godotengine/godot/master/doc/class.xsd">
	<brief_description>
		A filter for querying transactions.
	</brief_description>
	<description>
		This class is used to define criteria for filtering transactions. You can filter by caller, contract address, entrypoints, block range, and more.
	</description>
	<tutorials>
	</tutorials>
	<members>
		<member name="caller_addresses" type="String[]" setter="set_caller_addresses" getter="get_caller_addresses" default="[]">
			An array of caller addresses to filter by.
		</member>
		<member name="contract_addresses" type="String[]" setter="set_contract_addresses" getter="get_contract_addresses" default="[]">
			An array of contract addresses to filter by.
		</member>
		<member name="entrypoints" type="String[]" setter="set_entrypoints" getter="get_entrypoints" default="[]">
			An array of entrypoint selectors to filter by.
		</member>
		<member name="from_block" type="int" setter="set_from_block" getter="get_from_block" default="0">
			The starting block number for the filter.
		</member>
		<member name="model_selectors" type="String[]" setter="set_model_selectors" getter="get_model_selectors" default="[]">
			An array of model selectors to filter by.
		</member>
		<member name="to_block" type="int" setter="set_to_block" getter="get_to_block" default="0">
			The ending block number for the filter.
		</member>
		<member name="transaction_hashes" type="String[]" setter="set_transaction_hashes" getter="get_transaction_hashes" default="[]">
			An array of transaction hashes to filter by.
		</member>
	</members>
</class>
