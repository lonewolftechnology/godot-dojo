<?xml version="1.0" encoding="UTF-8" ?>
<class name="DojoActivityQuery" inherits="DojoQueryBase" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/godotengine/godot/master/doc/class.xsd">
	<brief_description>
		A query for retrieving activities.
	</brief_description>
	<description>
		This class is used to query for activities. It allows filtering by world addresses, caller addresses, namespaces and time range.
	</description>
	<tutorials>
	</tutorials>
	<members>
		<member name="caller_addresses" type="String[]" setter="set_caller_addresses" getter="get_caller_addresses" default="[]">
			An array of caller addresses to filter by.
		</member>
		<member name="from_time" type="DojoOptionU64" setter="set_from_time" getter="get_from_time">
			The start of the time range to query.
		</member>
		<member name="namespaces" type="String[]" setter="set_namespaces" getter="get_namespaces" default="[]">
			An array of namespaces to filter by.
		</member>
		<member name="to_time" type="DojoOptionU64" setter="set_to_time" getter="get_to_time">
			The end of the time range to query.
		</member>
		<member name="world_addresses" type="String[]" setter="set_world_addresses" getter="get_world_addresses" default="[]">
			An array of world addresses to filter by.
		</member>
	</members>
</class>
