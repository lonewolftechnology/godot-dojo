<?xml version="1.0" encoding="UTF-8" ?>
<class name="StarknetSubscription" inherits="DojoSubscription" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/godotengine/godot/master/doc/class.xsd">
	<brief_description>
		A subscription for raw Starknet events.
	</brief_description>
	<description>
		This class is used to subscribe to raw Starknet events. It allows filtering by event keys and the models that emit them.
	</description>
	<tutorials>
	</tutorials>
	<members>
		<member name="keys" type="Array" setter="set_keys" getter="get_keys" default="[]">
			An array of event keys to filter by.
		</member>
		<member name="models" type="String[]" setter="set_models" getter="get_models" default="[]">
			An array of model names to filter by.
		</member>
		<member name="name" type="String" setter="set_name" getter="get_name" overrides="DojoSubscription" default="&quot;StarknetSubscription&quot;" />
		<member name="pattern_matching" type="int" setter="set_pattern_matching" getter="get_pattern_matching" enum="dojo_bindings.PatternMatching" default="0">
			The pattern matching strategy to use for the keys. See [enum PatternMatching].
		</member>
		<member name="type" type="int" setter="" getter="get_type" overrides="DojoSubscription" enum="DojoSubscription.Type" default="7" />
	</members>
	<constants>
		<constant name="DOJO::PatternMatching::FixedLen" value="0" enum="PatternMatching">
			Fixed-length pattern matching.
		</constant>
		<constant name="DOJO::PatternMatching::VariableLen" value="1" enum="PatternMatching">
			Variable-length pattern matching.
		</constant>
	</constants>
</class>
